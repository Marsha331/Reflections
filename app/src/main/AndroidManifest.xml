<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="net.swallowsnest.reflections">

    <application
        android:allowBackup="true"
        android:debuggable="false"
        android:icon="@drawable/reflections_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LitActivity">
        </activity>
        <activity
            android:name=".DanceActivity">
        </activity>
        <activity
            android:name=".FilmActivity">
        </activity>
        <activity
            android:name=".MusicActivity">
        </activity>
        <activity
            android:name=".PhotoActivity">
        </activity>
        <activity
            android:name=".VisualActivity"></activity>
        <activity android:name=".RulesActivity"></activity>

    </application>

</manifest>